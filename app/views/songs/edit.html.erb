<h3>Song Form</h3>
<%= form_for(@song) do |f| %>

  <%= f.label "Song Name:" %><br>
  <%= f.text_field :title %><br>

  <%= f.label "Released:" %><br>
  <%= f.check_box :released %><br>

  <%= f.label "Release Year:" %><br>
  <%= f.number_field :release_year %><br>

  <%= f.label "Artist Name:" %><br>
  <%= f.text_field :artist_name %><br>

  <%= f.label "Genre:" %><br>
  <%= f.text_field :genre %><br>


  <%= f.submit %>

  <% end %>